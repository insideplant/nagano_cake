<h1>ITEM Index</h1>
<p>Find me in app/views/admin/products/index.html.erb</p>

<% @items.each do |item| %>
 <div>
  <%= attachment_image_tag item, :image %>
  <p>ジャンル<%= item.genre_id %></p>
  <p>商品名<%= item.name %></p>
  <p>価格<%= item.price %></p>
  <p>説明<%= item.introduction %></p>
 </div>
<% end %>