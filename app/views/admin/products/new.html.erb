<h1>Admin::Products#new</h1>
<p>Find me in app/views/admin/products/new.html.erb</p>

<%= form_with model: @item, local:true do |f| %>

<h4>ジャンル</h4>
 <%= :genre_id%>

<h4>商品名</h4>
 <%= f.text_field :name %>

<h4>画像投稿</h4>
 <%= f.attachment_field :image %>

<h4>説明</h4>
 <%= f.text_area :introduction %>

<h4>価格</h4>
 <%= f.text_field :price %>

<% end %>